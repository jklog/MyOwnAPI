{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"tymon/jwt-auth": "0.5.*",
		"rmccue/requests": "> 1.0",
		"dinesh/barcode": "^4.0",
		"jbdemonte/barcode": "^2.1",
		"bitgrave/barcode-bundle": "^0.9.7",
		"hackzilla/barcode-bundle": "^1.4",
		"druidvav/neko-barcode": "^1.1",
		"milon/barcode": "^0.5.0",
		"a2way/laravel-app-settings": "^1.0",
		"a2way/laravel-tenant-migrate": "1.*",
		"itsgoingd/clockwork": "^1.7",
		"barryvdh/laravel-debugbar": "^2.0",
		"mitulgolakiya/laravel-api-generator": "^1.3"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"fzaninotto/faker": "~1.4"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
